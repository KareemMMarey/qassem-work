                                    </div>
                                    <div class="pc-nav-header-menu-item">
                                        <img src="~/icons/papers.svg" alt="my requests" width="24" height="24">
                                        <a asp-controller="Request" asp-action="AllRequests" class="pc-nav-link"> @localizer["AllRequests"]</a>
                                    </div>
                                    <div class="pc-nav-header-menu-item">
                                        <img src="~/icons/papers.svg" alt="my requests" width="24" height="24">
                                        <a asp-controller="LookupOption" asp-action="Index" class="pc-nav-link"> @localizer["LookupOptions"]</a>
                                    </div>
                                    <div class="pc-nav-header-menu-item">
                                        <img src="~/icons/papers.svg" alt="my requests" width="24" height="24">
                                        <a asp-controller="News" asp-action="Index" class="pc-nav-link"> @localizer["News"]</a>
                                    </div>
