@* @using Framework.Identity.Data.Entities
@using Microsoft.AspNetCore.Identity
@using System.Globalization
@inject UserManager<ApplicationUser> _userManager
<!DOCTYPE html>
<html lang="ar" dir="rtl">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />


    <title>@ViewData["Title"] </title>
    <link rel="apple-touch-icon" sizes="57x57" href="/assets/images/fav/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/assets/images/fav/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/assets/images/fav/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/assets/images/fav/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/assets/images/fav/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/assets/images/fav/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/assets/images/fav/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/assets/images/fav/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/fav/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/assets/images/fav/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/images/fav/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="/assets/images/fav/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/images/fav/favicon-16x16.png">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;600&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://getbootstrap.com/docs/5.2//assets/css/docs.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">
    <link rel="stylesheet" href="/assets/css/main.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
</head>
<body>

    <div class="top-bar">
        <div class="container top-bar__container">
            <div class="top-bar__date">
                <svg width="24" height="24">
                    <use xlink:href="/assets/images/svg-icons.svg#date"> </use>
                    @{

                        CultureInfo CurrentCulture = CultureInfo.GetCultureInfo("ar-SA");
                        string text = DateTime.Now.ToString("dddd dd MMMM yyyy", CurrentCulture) + " هـ ";
                    }
                </svg> @text
            </div>
            <div class="top-bar__lang"> </div>
        </div>
    </div>
    <header class="header">
        <div class="container header__container">
            <a class="header__logo" href="/"><img src="/assets/images/qassim-logo.svg" hidden alt="إمارة منطقة القصيم"></a>
            <div class="header__nav-bar">
                <nav class="nav-bar">
                    <div class="nav-bar__item">
                        <div class="mega-menu">
                            <div class="container mega-menu__container">
                                <div class="row">
                                    <div class="col-4">
                                        <a class="mega-menu__item" href="~/Home/About">
                                            <div class="mega-menu__icon">
                                                <svg width="40" height="40">
                                                    <use xlink:href="/assets/images/svg-icons.svg#mega-about-icon1"> </use>
                                                </svg>
                                            </div>
                                            <div class="mega-menu__txt">
                                                <div class="mega-menu__title">
                                                    إمارة منطقة القصيم
                                                </div>
                                                <div class="mega-menu__description">
                                                    تعرف على إمارة منطقة القصيم
                                                </div>
                                            </div>
                                            <svg class="mega-menu__arrow" width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                            </svg>
                                        </a>
                                    </div>

                                </div>
                            </div>
                        </div><span hidden class="nav-bar__menu">عن الإمارة</span>
                    </div>
                    <div class="nav-bar__item"><a class="nav-bar__menu" href="/">الخدمات الإلكترونية</a></div>
                    <div class="nav-bar__item"><a class="nav-bar__menu" href="https://www.alqassim.gov.sa/EParticipation/"> المشاركة الالكترونية</a></div>
                    <div class="nav-bar__item">
                        <div class="mega-menu">
                            <div class="container mega-menu__container">
                                <div class="row">
                                    <div class="col-4">
                                        <a class="mega-menu__item" href="~/OpenData/Create">
                                            <div class="mega-menu__icon">
                                                <svg width="40" height="40">
                                                    <use xlink:href="/assets/images/svg-icons.svg#mega-media-icon1"> </use>
                                                </svg>
                                            </div>
                                            <div class="mega-menu__txt">
                                                <div class="mega-menu__title">
                                                    البيانات المفتوحة
                                                </div>
                                                <div class="mega-menu__description">
                                                    البيانات المفتوحة

                                                </div>
                                            </div>
                                            <svg class="mega-menu__arrow" width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                            </svg>
                                        </a>
                                    </div>


                                </div>
                            </div>
                        </div><span class="nav-bar__menu">البيانات المفتوحة</span>
                    </div>
                    <div class="nav-bar__item">
                        <div class="mega-menu">
                            <div class="container mega-menu__container">
                                <div class="row">
                                    <div class="col-4">
                                        <a class="mega-menu__item" href="~/Contact/Create">
                                            <div class="mega-menu__icon">
                                                <svg width="40" height="40">
                                                    <use xlink:href="/assets/images/svg-icons.svg#mega-social-icon1"> </use>
                                                </svg>
                                            </div>
                                            <div class="mega-menu__txt">
                                                <div class="mega-menu__title">
                                                    تواصل مع الإمارة
                                                </div>
                                                <div class="mega-menu__description">
                                                    تواصل مع الإمارة
                                                </div>
                                            </div>
                                            <svg class="mega-menu__arrow" width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                            </svg>
                                        </a>
                                    </div>
                                    <div class="col-4">
                                        <a class="mega-menu__item" href="~/ShareData/Create">
                                            <div class="mega-menu__icon">
                                                <svg width="40" height="40">
                                                    <use xlink:href="/assets/images/svg-icons.svg#mega-social-icon2"> </use>
                                                </svg>
                                            </div>
                                            <div class="mega-menu__txt">
                                                <div class="mega-menu__title">
                                                    مشاركة البيانات
                                                </div>
                                                <div class="mega-menu__description">
                                                    طلبات مشاركة البيانات
                                                </div>
                                            </div>
                                            <svg class="mega-menu__arrow" width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                            </svg>
                                        </a>
                                    </div>

                                </div>
                            </div>
                        </div><span hidden class="nav-bar__menu">المشاركة المجتمعية</span>
                    </div>
                    <div class="nav-bar__item"><a class="nav-bar__menu" href="~/ShareData/Create"> مشاركة البيانات</a></div>

                </nav>
            </div>
            <div class="header__login">
                <div class="header-login">
                    @if (!User.Identity.IsAuthenticated)
                    {

                        <a class="btn btn-outline-primary" href="~/Account/NafathLogin">تسجيل الدخول</a>
                    }
                    else
                    {
                        var user = await _userManager.FindByNameAsync(User.Identity.Name);
                        <div class="d-block" id="userMenu">
                            <div class="dropdown header-login__dropdown">
                                <button class="dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    <svg width="24" height="24">
                                        <use xlink:href="/assets/images/svg-icons.svg#user-circle"> </use>
                                    </svg>مرحبا
                                    @if (!string.IsNullOrEmpty(user.FullName))
                                    {
                                        <span>@user.FullName</span>
                                    }

                                    else
                                    {
                                        <span>@User.Identity.Name</span>
                                    }

                                </button>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a class="dropdown-item" href="~/Account/Profile">
                                            <svg width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#user" />
                                            </svg> ملفي الشخصي
                                        </a>
                                    </li>
                                    @if (User.IsInRole("Admin"))
                                    {
                                        <li>
                                            <a class="dropdown-item" href="~/Configuration">
                                                <svg width="24" height="24">
                                                    <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                                </svg> إدارة الخدمات
                                            </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="~/RequestsAdmin/RequestList">
                                                <svg width="24" height="24">
                                                    <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                                </svg> طلبات الخدمات
                                            </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="~/Account/Members">
                                                <svg width="24" height="24">
                                                    <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                                </svg>المستخدمين
                                            </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="~/Roles/List">
                                                <svg width="24" height="24">
                                                    <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                                </svg>الأدوار
                                            </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="~/Roles/Assign">
                                                <svg width="24" height="24">
                                                    <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                                </svg>صلاحيات
                                            </a>
                                        </li>
                                    }
                                    <li>
                                        <a class="dropdown-item" href="~/Home/MyRequests">
                                            <svg width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#orders" />
                                            </svg> طلباتي
                                        </a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item dropdown-item--logout" href="~/Account/Logout">
                                            <svg width="24" height="24">
                                                <use xlink:href="/assets/images/svg-icons.svg#logout" />
                                            </svg> تسجيل الخروج
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    }

                </div>
            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="page-layout">
            @RenderBody()
        </main>
    </div>



    <footer class="footer">
        <div class="container">
            <div class="row footer__row">
                <div class="col-xl-4 footer__col">
                    <h4 class="footer__title">تصفح</h4>
                    <ul class="footer__list">

                        <li class="footer__li">
                            <a class="footer__link" target="_blank" href="~/assets/Privacy.pdf">
                                سياسة الخصوصية
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                </svg>
                            </a>
                        </li>
                        <li class="footer__li">
                            <a class="footer__link" target="_blank" href="~/assets/SLA.pdf">
                                اتفاقية مستوى الخدمة
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                </svg>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="col-xl-4 footer__col">
                    <h4 class="footer__title">روابط خارجية</h4>
                    <ul class="footer__list">
                        <li class="footer__li">
                            <a class="footer__link" asp-action="Create" asp-controller="Contact">
                                منصة صوت المواطن
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#external-link"> </use>
                                </svg>
                            </a>
                        </li>
                        <li class="footer__li">
                            <a class="footer__link" href="/">
                                خدمات الإمارة في أبشر
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#external-link"> </use>
                                </svg>
                            </a>
                        </li>

                        <li class="footer__li">
                            <a class="footer__link" href="~/Home/About">
                                اكتشف القصيم
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#external-link"> </use>
                                </svg>
                            </a>
                        </li>

                    </ul>
                </div>
                <div class="col-xl-4 footer__col">
                    <h4 class="footer__title">تواصل</h4>
                    <ul class="footer__list">
                        <li class="footer__li">
                            <a class="footer__link" href="/Contact/Create">
                                تواصل معنا
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                </svg>
                            </a>
                        </li>
                        <li class="footer__li">
                            <a class="footer__link" href="/Contact/Create">
                                اقتراحات
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                </svg>
                            </a>
                        </li>
                        <li class="footer__li">
                            <a class="footer__link" href="/Contact/Create">
                                دعم فني
                                <svg>
                                    <use xlink:href="/assets/images/svg-icons.svg#left-arrow"> </use>
                                </svg>
                            </a>
                        </li>
                    </ul>
                    <ul class="footer__apps">
                        <li class="footer__apps-li">
                            <a class="footer__apps-link" href="#" target="_blank">
