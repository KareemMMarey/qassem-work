 *@
 @inject UserManager<ApplicationUser> _userManager
 @inject IViewLocalizer localizer
 @using System.Globalization
 @using Microsoft.AspNetCore.Authentication;

@{
    // Determine the page direction based on the current culture
    var direction = CultureInfo.CurrentUICulture.Name.StartsWith("ar") ? "rtl" : "ltr";
}

<!DOCTYPE html>
<html lang="@CultureInfo.CurrentUICulture.Name" dir="@direction">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] إمارة منطقة القصيم</title>
    <link rel="shortcut icon" type="x-icon" href="~/images/emarahCircleLogoWhite.png" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/QassimPrincipality.Web.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans+Arabic:wght@100;200;300;400;500;600;700&display=swap" rel="stylesheet">
</head>
<body>
    <header class="pc-header">
        <div class="pc-header-date-time">
            <div>
                <img src="~/icons/date.svg" alt="date" width="20.5" height="21.5">
                <p id="current-date"></p>
            </div>
            <div>
                <img src="~/icons/time.svg" alt="time" width="21.5" height="21.5">
                <p id="current-time"></p>
            </div>
        </div>

        @* desktop nav*@
        <nav class="pc-nav-header">
            <a asp-controller="Home" asp-action="Index"> <img src="~/images/emarahColoredLogo.svg" alt="logo" width="149" height="74"></a>

            <div class="pc-nav-bar">
                <div class="pc-nav-bar-group">
                    <div class="pc-nav-tab">
                        <a class="pc-nav-link" asp-controller="Home" asp-action="About"> @localizer["About"] </a>
                    </div>

                    <div class="pc-nav-tab">
                        <a class="pc-nav-link" asp-controller="Services" asp-action="Index"> @localizer["EService"] </a>
                    </div>

                    <div class="pc-nav-tab">
                        <a class="pc-nav-link" asp-controller="EParticipation" asp-action="Index"> @localizer["EParticipation"] </a>
                    </div>
                </div>

                <div class="pc-nav-bar-group">
                    @* later versions *@
                    @* <div class="pc-nav-tab">
                    <img src="~/icons/translation.svg" alt="translation" width="24" height="24">
                    <a class="pc-nav-link" href="#">English</a>
                    </div> *@
                    @if (!User.Identity.IsAuthenticated)
                    {

                        @* <a class="btn btn-outline-primary" href="~/Account/NafathLogin">تسجيل الدخول</a> *@
                        @* loggedout *@
                        <div class="pc-nav-tab">
                            <img src="~/icons/login.svg" alt="login" width="24" height="24">
                            <a class="pc-nav-link" asp-controller="Account" asp-action="NafathLogin">@localizer["Login"]</a>
                        </div>
                    }
                    else
                    {


                        var user = await _userManager.FindByNameAsync(User.Identity.Name);
                        @* logged in *@
                        <div class="pc-nav-dropdown-container">
                            <div class="pc-nav-tab pc-nav-dropdown-tab">
                                <img src="~/icons/user.svg" alt="login" width="24" height="24">
                                @if (!string.IsNullOrEmpty(user.FullName))
                                {

                                    <p class="pc-nav-link">@user.FullName</p>
                                }

                                else
                                {

                                    <p class="pc-nav-link">@user.UserName</p>
                                }

                                <div class="pc-arrow-toggle">
                                    <img src="~/icons/arrowDown.svg" alt="down arrow" class="pc-down-arrow" width="14" height="8">
                                    <img src="~/icons/arrowUp.svg" alt="up arrow" class="pc-up-arrow" width="16" height="16">
                                </div>
                            </div>

                            <div class="pc-nav-dropdown">
                                <div class="pc-nav-header-menu-item">
                                    <img src="~/icons/userCircle.svg" alt="profile" width="24" height="24">
                                    <a asp-controller="Account" asp-action="Profile" class="pc-nav-link"> @localizer["MyProfile"]</a>
                                </div>

                                <div class="pc-nav-header-menu-item">
                                    <img src="~/icons/papers.svg" alt="my requests" width="24" height="24">
